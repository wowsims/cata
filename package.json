{
	"name": "cata",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"npm": "Please use Yarn",
		"node": ">= 18"
	},
	"volta": {
		"node": ">= 18",
		"npm": ">= 9"
	},
	"scripts": {
		"build": "bazel build //...",
		"test": "bazel test //...",
		"format": "npm run lint:fix && npm run prettier:fix",
		"lint": "npm run lint:js && npm run lint:css",
		"lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
		"lint:js": "npx eslint \"./ui/**/*.{js,jsx,ts,tsx}\"",
		"lint:js:fix": "npm run lint:js -- --fix",
		"lint:css": "stylelint \"./ui/**/*.scss\"",
		"lint:css:fix": "npm run lint:css -- --fix",
		"lint:json": "npx eslint \"./{ui,jsonschema}/**/*.json\"",
		"lint:json:fix": "npm run lint:json -- --fix",
		"prettier": "npx prettier \"./ui/**/*.{js,jsx,ts,tsx,scss,css,json}\" --check",
		"prettier:fix": "npm run prettier -- --write",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.6",
		"@types/pako": "^2.0.0",
		"@wowsims/assets": "https://github.com/wowsims/wowsims.git",
		"@wowsims/config": "https://github.com/wowsims/wowsims.git",
		"@wowsims/constants": "https://github.com/wowsims/wowsims.git",
		"@wowsims/ui": "https://github.com/wowsims/wowsims.git",
		"bootstrap": "^5.3.0",
		"pako": "^2.0.4",
		"tippy.js": "^6.3.7",
		"tsx-vanilla": "^1.0.0"
	},
	"devDependencies": {
		"@protobuf-ts/plugin": "2.9.1",
		"@protobuf-ts/plugin-framework": "2.9.1",
		"@protobuf-ts/protoc": "2.9.1",
		"@protobuf-ts/runtime": "2.9.1",
		"@types/bootstrap": "^5.2.0",
		"@types/node": "^18.16.1",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"clsx": "^2.1.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"glob": "^8.0.3",
		"http-server": "13.0.1",
		"postcss-scss": "^4.0.9",
		"prettier": "3.0.3",
		"sass": "^1.64.2",
		"stylelint": "^15.10.3",
		"stylelint-prettier": "^4.0.2",
		"stylelint-scss": "^5.2.1",
		"terser": "^5.14.2",
		"typescript": "5.2.2",
		"typescript-formatter": "^7.2.2",
		"vite": "^5.0.0"
	},
	"resolutions": {
		"@wowsims/assets": "portal:../wowsims-mono/packages/assets",
		"@wowsims/config": "portal:../wowsims-mono/packages/config",
		"@wowsims/constants": "portal:../wowsims-mono/packages/constants",
		"@wowsims/ui": "portal:../wowsims-mono/packages/ui",
		"@wowsims/core": "portal:../wowsims-mono",
		"@wowsims/styleguide": "portal:../wowsims-mono/apps/styleguide"
	},
	"packageManager": "yarn@3.4.1"
}
